$SUBROUTINE ADVAN3 TRANS4

$PK
;---- FIXED EFFECT DEFINITION ---- 
 TVCL = THETA(3)
 TVV1 = THETA(4)
 TVV2 = THETA(5)
 TVQ  = THETA(6)
 TVD1 = THETA(7)
 
;---- RANDOM EFFECT DEFINITION ----

 CL   = TVCL * EXP(ETA(1))
 V1   = TVV1 * EXP(ETA(2))
 V2   = TVV2 * EXP(ETA(3))
 Q    = TVQ  * EXP(ETA(4))
 D1   = TVD1 * EXP(ETA(5))
 F1   = THETA(8) - THETA(10) * DOSE / (THETA(9) + DOSE) * EXP(ETA(6))
 S1 = V1

$ERROR
 IPRED  = F
 W      = SQRT(THETA(1)**2 + THETA(2)**2 * IPRED**2)
 IRES   = DV - IPRED
 IWRES  = IRES / W
 Y      = IPRED + W * EPS(1)

